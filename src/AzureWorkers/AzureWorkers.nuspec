      <dependency id="WindowsAzure.ServiceBus" version="2.4" />
      <dependency id="WindowsAzure.Storage" version="4.2"/>
      <dependency id="EnterpriseLibrary.TransientFaultHandling" version="6.0"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\bin\AzureWorkers.dll" target="lib\net451" />
